{
    "require": {
        "php": "~7.4.0 || ~8.0.0 || ~8.1.0",
        "knplabs/gaufrette": "0.10.0",
        "pimple/pimple": "3.5.0",
        "slim/slim": "2.6.3",
        "symfony/console": "~4.4.34",
        "symfony/finder": "~4.4.30",
        "symfony/polyfill-php80": "^1.23",
        "symfony/polyfill-php81": "^1.23"
    },
    "autoload": {
        "psr-4": {
            "Shopware\\Recovery\\Common\\": "src",
            "Shopware\\Recovery\\Install\\": "../install/src",
            "Shopware\\Recovery\\Update\\": "../update/src"
        },
        "files": ["functions.php"]
    },
    "replace": {
        "paragonie/random_compat": "*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-php72": "*"
    },
    "config": {
        "autoloader-suffix": "ShopwareRecovery",
        "sort-packages": true
    }
}
